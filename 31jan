    int minRepeats(string A, string B) {
        // code here
        using ll = int;

        ll i, j, c = 1;

        string str = A;

        while(A.find(B) == string::npos && A.length() <= B.length()){

            A+=str;

            c++;

        }

        if(A.find(B) == string::npos){

            return -1;

        }

        

        return c;
        
    }
